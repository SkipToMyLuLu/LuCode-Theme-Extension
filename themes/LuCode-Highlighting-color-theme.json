{
	"name": "LuCode-Highlighting",
	"colors": {
		"editor.background": "#263238",
		// "editor.foreground": "#eeffff",
		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#bbbbbb"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"string.comment"
			],
			"settings": {
				"foreground": "#00792681",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment.lucode"
			],
			"settings": {
				"foreground": "#9cd810",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Syntax",
			"scope": [
				"syntax.lucode"
			],
			"settings": {
				"foreground": "#2200ffcf",
				"fontStyle": "bold"
			}
		}
	]
}