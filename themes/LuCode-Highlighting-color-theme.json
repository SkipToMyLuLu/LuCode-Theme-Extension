{
	"name": "LuCode-Highlighting",
	"colors": {
		"editor.background": "#263238",
		"editor.foreground": "#eeffff",
		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#bbbbbb"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"string.comment"
			],
			"settings": {
				"foreground": "#00792681",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable",
				"variable.other",
				"variable.parameter"
			],
			"settings": {
				"foreground": "#94cb13ce",
				"fontStyle": "italic"
			}
		}
	]
}